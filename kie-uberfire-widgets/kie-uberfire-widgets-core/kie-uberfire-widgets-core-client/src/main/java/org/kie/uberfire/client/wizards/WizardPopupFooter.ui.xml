<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
  xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'>

  <ui:with field="i18n" type="org.kie.uberfire.client.resources.i18n.CoreConstants" />

      <b:ModalFooter>
        <b:Button ui:field="btnPrevious" icon="PLUS_SIGN" text="{i18n.Previous}"></b:Button>
        <b:Button ui:field="btnNext" icon="PLUS_SIGN" text="{i18n.Next}"></b:Button>
        <b:Button ui:field="btnCancel" icon="PLUS_SIGN" text="{i18n.cancel}"></b:Button>
        <b:Button ui:field="btnFinish" icon="PLUS_SIGN" type="PRIMARY" text="{i18n.Finish}"></b:Button>
      </b:ModalFooter>

</ui:UiBinder>