<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:with field='res' type='org.uberfire.ext.apps.client.resources.WebAppResource'/>
  <g:FlowPanel ui:field="outerPanel" styleName="{res.CSS.tile}">
    <g:FlowPanel ui:field="deletePanel" styleName="{res.CSS.deletePanel}"/>
    <g:FlowPanel ui:field="tilePanel" styleName="{res.CSS.tileContent}">
      <g:FlowPanel styleName="{res.CSS.iconPanel}">
        <b:Icon type="FILE" ui:field="icon" styleName="fa {res.CSS.tileIcon}"/>
      </g:FlowPanel>
    </g:FlowPanel>
    <g:FlowPanel styleName="{res.CSS.tileStatus}">
      <b:Label styleName="{res.CSS.tileName}" ui:field="label"/>
    </g:FlowPanel>
  </g:FlowPanel>

</ui:UiBinder>
