<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:HTMLPanel>
        <b:InputGroup ui:field="filterGroup">
            <b:TextBox ui:field="filterBox" placeholder="filter properties..."/>
            <b:InputGroupButton>
                <b:Button icon="RETWEET" ui:field="reload"/>
            </b:InputGroupButton>
        </b:InputGroup>
        <b:PanelGroup ui:field='propertyMenu'/>
    </g:HTMLPanel>
</ui:UiBinder>